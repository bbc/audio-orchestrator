{
  "appId": "uk.co.bbc.rd.bbcat-orchestration-builder",
  "copyright": "Copyright Â© 2020 BBC R&D",
  "productName": "BBC R&D Audio Orchestrator",
  "files": [
    "./credits.html",
    "./bbcrd-logo.svg",
    "./bin/**/*",
    "!**/node_modules/@bbc/bbcat-*/src/**/*",
    "**/node_modules/@bbc/bbcat-orchestration-template/dist/**/*"
  ],
  "asar": {
    "smartUnpack": false
  },
  "asarUnpack": [
    "**/node_modules/@bbc/bbcat-orchestration-template/**/*"
  ],
  "removePackageScripts": false,
  "dmg": {
    "icon": null
  },
  "mac": {
    "hardenedRuntime": true,
    "entitlements": "build/entitlements.mac.plist",
    "entitlementsInherit": "build/entitlements.mac.plist",
    "gatekeeperAssess": false
  }
}
