{
  "extends": [
    "../.eslintrc",
    "plugin:jest/recommended"
  ],
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": "latest"
  },
  "plugins": ["jest"],
  "env": {
    "jest/globals": true
  },
  "rules": {
    "import/extensions": ["error", "ignorePackages"],
    "import/no-extraneous-dependencies": 0,
    "jest/no-conditional-expect": 1
  }
}
